generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User{
  id  String  @id @default(cuid())
  email String  @unique
  name  String
  verify  Verify[]
}
model Verify{
  id String @id @default(cuid())
  email String
  mobilenumber  String
  verify  Boolean?
  user  User  @relation(fields: [email], references: [email])
}
model Docktor{
  id String @id @default(cuid())
  name String
  spaclist  String
  aderess String
  startTime String
  endTime String
}